User Function zEX001()
    LOCAL aPergs     := {}
    LOCAL dDateIn    := FirstDate(Date())
    LOCAL dDateUntil := LastDate(Date())

    aAdd(aPergs, { 1, "Data De?", dDateIn, "", ".T.", "", ".T.", 80, .F. })
    aAdd(aPergs, { 1, "Data Até?", dDateUntil, "", ".T.", "", ".T.", 80, .F. })
    
    

    If ParamBox(aPergs, "Informe os Paramentros")
        Alert(MV_PAR01)
        Alert(MV_PAR02)
    EndIf

Return
